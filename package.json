{
  "name": "optionsviz",
  "version": "2.0.0",
  "description": "Options Visualization Tool - Full TypeScript Stack",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd backend && npm run dev",
    "backend:build": "cd backend && npm run build",
    "backend:start": "cd backend && npm start",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "npm run backend:build && npm run frontend:build",
    "start": "npm run backend:start",
    "clean": "cd backend && npm run clean"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["options", "trading", "visualization", "typescript", "react", "express"],
  "author": "Options Visualization Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}

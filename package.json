{
  "name": "optionsviz",
  "version": "2.0.0",
  "description": "Options Visualization Tool - Unified TypeScript Stack",
  "type": "module",
  "main": "dist/server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "dev:frontend": "vite",
    "client:dev": "vite",
    "client:build": "vite build",
    "client:preview": "vite preview",
    "server:dev": "tsx watch src/server/server.ts",
    "server:build": "tsc --project tsconfig.server.json",
    "build": "npm run server:build && npm run client:build",
    "build:frontend": "vite build",
    "start": "node dist/server/server.js",
    "start:frontend": "vite preview --port 3000",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "install:all": "npm install"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "lightweight-charts": "^4.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/sqlite3": "^3.1.11",
    "@vitejs/plugin-react": "^4.1.0",
    "concurrently": "^8.2.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vite": "^7.1.2"
  },
  "keywords": [
    "options",
    "trading",
    "visualization",
    "typescript",
    "react",
    "express"
  ],
  "author": "Options Visualization Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
